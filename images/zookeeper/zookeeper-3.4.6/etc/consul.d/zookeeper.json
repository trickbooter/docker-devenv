{
    "service": {
        "name": "zookeeper",
        "port": 2181,
        "tags" : ["v3.4.2"],
        "check": {
            "name": "Zookeeper Check",
            "notes": "Connect to the port internally every 10 seconds",
            "script": "nc -z -v localhost 2181 >/dev/null 2>&1",
            "interval": "10s"
        }
    }
}
